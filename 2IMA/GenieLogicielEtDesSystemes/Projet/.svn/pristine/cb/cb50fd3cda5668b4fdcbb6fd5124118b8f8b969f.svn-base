public class Panneau extends JPanel implements KeyListener {

	private Graphics g;
	private String fond = "src/fond.jpg";
	public Panneau(){
		addKeyListener(this);
		
	}
	
	
	
	public void paintComponent(Graphics g){
		Image img;
		try {
			img = ImageIO.read(new File(fond));
			g.drawImage(img, 0, 0, 800, 600, this);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		repaint();
	}
	
	
