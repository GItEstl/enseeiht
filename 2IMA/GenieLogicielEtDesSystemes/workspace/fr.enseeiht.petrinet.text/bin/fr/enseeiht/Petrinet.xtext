grammar fr.enseeiht.Petrinet with org.eclipse.xtext.common.Terminals

import "http://petrinet" as petri
import "http://www.eclipse.org/emf/2002/Ecore" as ecore

Petrinet returns petri::PetriNet :
	'petrinet' name=ID '{'
	node+=(Place|Transition)*
	arc+=Arc*
	'}'
	;

Place returns petri::Place :
	'pl' name=ID nbJetons=INT
	;
		
Transition returns petri::Transition :
	'tr' name=ID
	;
	
Arc returns petri::Arc :
	'from' target=[petri::Node]
	'to' source=[petri::Node]
	'weight' poids=INT
	;
